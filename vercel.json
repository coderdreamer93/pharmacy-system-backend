{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "env": {
          "NODE_ENV": "production"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api/*",
      "dest": "api/index.js",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}